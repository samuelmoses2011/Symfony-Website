{% extends '@IIABMagnet/Admin/standard_layout.html.twig' %}

{% block title %}
    Send Customized Notification
{% endblock%}

{% block navbar_title %}
    {{ block('title') }}
{% endblock %}

{% block sonata_admin_content %}
    <div class="sonata-ba-form">
        <section class="content-header">
            <nav role="navigation" class="navbar navbar-default">
                <div class="navbar-header"><span class="navbar-brand">Send Customized Notification</span></div>
            </nav>
        </section>
        <section class="content">
            <div class="sonata-ba-form">
                <div class="row">
                    <div class="col-md-12">
                        <div class="">
                            <div class="box box-success">
                                <div class="box-header">
                                    <h4 class="box-title">Customized Notification Email/Letter</h4>
                                </div>
                                <div class="box-body placement">
                                    {{ form(form) }}
                                    <div class="clear clearfix"></div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>
    </div>
{% endblock sonata_admin_content %}
